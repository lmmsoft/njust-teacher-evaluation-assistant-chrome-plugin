{
  "manifest_version": 2,
  "name": "南理工评教助手",
  "version": "1.0",
  "description": "加快评教的速度",
  "homepage_url": "http://njust.aliapp.com/pingjiao/",
  "background_page": "background.html",
  "content_scripts": [ {
      "css": [ "inject.css" ],
      "js": [ "inject.js" ],
      "matches": ["http://202.119.81.118/*"],
      "run_at": "document_end"
   } ],
  "browser_action": {
    "default_icon": "icon.png",
